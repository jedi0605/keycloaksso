# Server Configuration
PORT=3001

# Keycloak Configuration
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=master

# Client Credentials Authentication (preferred)
KEYCLOAK_CLIENT_ID=your-client-id
